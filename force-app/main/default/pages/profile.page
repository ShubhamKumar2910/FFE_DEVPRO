<apex:page showheader="false" sidebar="false" standardstylesheets="false" cache="false" doctype="html-5.0" lightningStylesheets="false">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1" />
    <style>
        md-select[disabled],md-datepicker[disabled]{
        PADDING-LEFT:0 !important;
        }
        .activetabs a{
        color: #fff !important; 
        }
        .headersHistory{
        margin-top:148px; 
        border: 1px solid #032559;
        background: #fff;
        width: 89.85% !important;
        }
        .activetabs { 
        background: #ff793c;
        } 
        .headersHistory ul li a{
        font-size: 14px;
        color: #032559;
        }
        .activetabs a{
        color: #fff !important; 
        }
        .switchDiv{
        top: 14px;
        position: relative;
        }
        .switchPages{
        position: relative;
        top: 20px;
        float: left;
        }
        .uploadDocsDiv{
        float: left;
        width: 100%;
        }
        md-select[disabled] .md-select-value {
        background-position: 0 bottom;
        background-size: 4px 1px;
        background-repeat: repeat-x;
        margin-bottom: -1px;
        background: #eee;
        border: 1px solid #ccc;
        }
        .rowsHistory th {
        color: #000;
        }
        .rowabout{
        width:91.5%;
        }
        .rowabout textarea{
        width:100%;
        }
        
        .mandatorCls{
        color:red;
        }
        .tooltip {
        position: relative;
        display: inline-block;
        border-bottom: 0px dotted black;
        }
        .missingDoc{
        width: 90%;
        float: left;
        min-height: 30px;
        border: 1px solid rgba(99, 97, 121, 0.49);
        border-radius: 5px;
        padding: 0;
        resize: none;
        padding-left: 8px;
        border-color: Red;
        }
        .profileCSS{
        	width:54%;float:left;
        }
        .appList{}
         @media screen and (max-width: 600px){
        		.appList{
        			display:flex;
        			flex-direction:row;
        		}
        		.headermenuSelection{
        			overflow:hidden;
        		}
        		.profileCSS{
        			width:100%;
        		}
        		.profileCSS{
        			width:50%;
        		}
        }
        
    </style>
    <script>
    window.onload=function(){
        var i;
        var x = document.getElementsByClassName("city");
        for (i = 0; i < x.length; i++) {
            if(i==0){
                x[i].style.display = "block";
            }
            else{
                x[i].style.display = "none";
            }
        }
    }
    </script>
    <body style="margin-top:50%;">
        
    
    <div class="headermenuSelection headersHistory" ng-init="switch(2,2);getListofFiles();">
        
        <ul style=""  class="profileCSS">
            <li ng-click="switch(0,0)" ng-class="{activetabs: 0 == selectedtabs}" onclick="openCity('Basic')" ><a  ng-click="moveToTop();">Profile Details</a></li>
            <li  ng-click="switch(2,2);getListofFiles();" ng-class="{activetabs: 2 == selectedtabs}" onclick="openCity('course')" ><a ng-click="moveToTop();">Applications</a></li>
            <li  ng-click="switch(3,3);" ng-if="!IsPrelimApplication" ng-class="{activetabs: 3 == selectedtabs}" onclick="openCity('tellYourStory')" ><a ng-click="moveToTop();">Tell Your Story</a></li>
        </ul>
        
        <div style="width:40%;float: left;padding: 9px 0;">
            <a ng-show="IsPrelimApplication" class="pdf" href="/application/scholar_profile?ScholarId={{studendId}}" target="_blank" style="float: left;/* position: relative; *//* top: 7px; *//* font-size: 11pt; *//* left: -299px; */color: rgb(66, 92, 170);/* text-decoration: underline; */cursor: pointer;width: 30%;text-align: right;">Generate Profile as Pdf</a>
            <!-- <a ng-show="IsPrelimApplication" href="{!$Setup.System_Details__c.Generate_Pledge_Parent_Declaration__c}" target="_blank" style="float: left;position: relative;/* top:7px; *//* font-size: 11pt; *//* margin-left: 10px; */text-align: left;color: #425caa;/* text-decoration: underline; */cursor: pointer;width: 61%;text-indent: 1em;">Generate the Pledge and Parent Declaration</a> -->
        </div>
    </div>
    <ng-form name="basicInfo" novalidate="true" class="switchDiv">
        
        <div id="Basic" class="city switchPages" ng-init="tabs();">
            
            <div class="buttonDiv" align="center" style="margin-bottom:0px;">
                <input type="button" value="Edit" class="buttons"  ng-click="edit();" ng-if="editFileds"/>
                <input type="button" value="Save" class="buttons"  ng-click="save();" ng-if="!editFileds"/>
                <input type="button" value="Cancel" class="buttons"  ng-click="cancelChanges();" ng-if="!editFileds"/>
            </div>
            <div class="formDiv">
                <h4 class="formheadings" align="center">
                    Basic Information 
                </h4>
                <div class="row">
                    <h4 class="tagnames" >
                        First Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="fName" ng-model="student.FirstName"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.fName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.fName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Last Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="userLastName" ng-model="student.LastName"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.userLastName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.userLastName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Gender:<span style="color:#ff0000">*</span>
                    </h4>
                    <select class="inputfiledsSelect" name="gender" style="width:92%;color:black;" ng-model="student.Gender__c" required='true' ng-disabled="editFileds">
                        <option  value='Male'>Male</option>
                        <option  value='Female'>Female</option>
                        <option  value='Other'>Other</option>
                    </select>
                    <span ng-show="formInvalid && basicInfo.gender.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.gender.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Email:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="userEmail" ng-model="student.Email"  class="inputfileds" ng-change="valid(student.Email)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.userEmail.$invalid" class="error">This is required.</span>
                    <span class="error" ng-show="(!(basicInfo.userEmail.$invalid) && invalidEmail)">Invalid email address</span>
                    <span ng-show="!(formInvalid && basicInfo.userEmail.$invalid) && !(!(basicInfo.userEmail.$invalid) && invalidEmail) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" >
                    <h4 class="tagnames" >
                        Date Of Birth:<span style="color:#ff0000">*</span>
                    </h4>
                    <md-datepicker style="min-height: 30px;" class="inputfiledsSelect" type="date" ng-model="student.Birthdate" required="true" name="userDob" id="mydate" ng-disabled="editFileds" md-max-date='todate'></md-datepicker>
                    <span ng-show="formInvalid && basicInfo.userDob.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.userDob.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                
                <div class="row" >
                    <h4 class="tagnames" >
                        Aadhaar No: <span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" ng-if="scholarshipGrantedAtLeastOnce || student.KYC_Verified__c" name="userAadhar" ng-model="student.Aadhar_number__c"  class="inputfileds"  ng-readonly='true'  />
                    <input type="text" ng-if="!scholarshipGrantedAtLeastOnce && !student.KYC_Verified__c" name="userAadhar" ng-model="student.Aadhar_number__c"  class="inputfileds"  onpaste="return isNumber(event)" onkeypress="return isNumber(event)" ng-readonly="editFileds || scholarshipGrantedAtLeastOnce || student.KYC_Verified__c"  required='true' maxlength="12" minlength="12"/> 
                    <span ng-show="formInvalid && basicInfo.userAadhar.$invalid && !student.KYC_Verified__c && !scholarshipGrantedAtLeastOnce" class="error">&nbsp;Enter your 12 digit Aadhaar number.</span>
                </div>
                
                
                <div class="row">
                    <h4 class="tagnames" >
                        Mobile No:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="userMobile" ng-model="student.MobilePhone"  class="inputfileds" onpaste="return isNumber(event)" onkeypress="return isNumber(event)" required='true' maxlength="11" minlength="10" ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.userMobile.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.userMobile.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Phone No:
                    </h4>
                    <input type="text" name="userPhone" ng-model="student.Phone"  class="inputfileds" onpaste="return isNumber(event)" onkeypress="return isNumber(event)" maxlength="11" minlength="6"  ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="(formInvalid && basicInfo.userMobile.$error.minlength || formInvalid && basicInfo.userMobile.$error.maxlength )" class="error">Phone Number length should be between 6-11 digits</span>
                </div>
                <div class="row" ng-if='editFileds'>
                    <h4 class="tagnames" >
                        Student Unique Id:<span style="color:#ff0000"></span>
                    </h4>
                    <input type="text" name="id" ng-model="student.FFE_ID__c"  class="inputfileds" ng-readonly="true"/>              
                </div>
                <div ng-if="editFileds">
                    <div class="row" ng-if="(student.Facilitator_Name__r.Name!=undefined && student.Facilitator_Name__r.Name!='')">
                        <h4 class="tagnames" >
                            Assigned Facilitator Name:
                        </h4>
                        <input type="text" name="Facilitator_Name__c" ng-model="student.Facilitator_Name__r.Name"  class="inputfileds readInput" onpaste="return isChar(event)" onkeypress="return isChar(event)" readonly='true'/>
                    </div>
                    
                    <div class="row" ng-if='editFileds'>
                        <h4 class="tagnames" >
                            Total Training Point(s) Earned: <span style="color:#ff0000"></span>
                        </h4>
                        <input type="text" name="id" ng-model="student.Total_Point_s__c"  class="inputfileds" ng-readonly="true"/>              
                    </div>
                    
                    <div class="row" ng-if="(student.Refered_by_Facilitator_Name__c!=undefined && student.Refered_by_Facilitator_Name__c!='')">
                        <h4 class="tagnames" >
                            Facilitator Name:
                        </h4>
                        <input type="text" name="Refered_by_Facilitator_Name__c" ng-model="student.Refered_by_Facilitator_Name__c"  class="inputfileds readInput" onpaste="return isChar(event)" onkeypress="return isChar(event)" readonly='true'/>
                    </div>
                    <div class="row" ng-if="(student.Refered_by_Facilitator_Id__c!=undefined && student.Refered_by_Facilitator_Id__c!='')">
                        <h4 class="tagnames" >
                            Facilitator Id:
                        </h4>
                        <input type="text" name="Refered_by_Facilitator_Id__c" ng-model="student.Refered_by_Facilitator_Id__c"  class="inputfileds readInput" onpaste="return isAlphaNumber(event)" onkeypress="return isAlphaNumber(event)" readonly='true'/>
                    </div>
                </div>
            </div>
            <div class="formDiv">
                <h4 class="formheadings" align="center">
                    Bank Information    
                </h4>
                <div class="row">
                    <h4 class="tagnames" >
                        Bank Account Holder's Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="acName" ng-model="student.Bank_Account_Holder_s_Name__c"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.acName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.acName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Full Bank Account No. (With all digits):<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="acNum" ng-model="student.Full_Bank_Account_No_With_all_digits__c"  class="inputfileds" onpaste="return isNumber(event)" onkeypress="return isNumber(event)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.acNum.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.acNum.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                
                
                <div class="row">
                    <h4 class="formTags" >
                        11 characters IFSC Code of the Branch:<span style="color:#ff0000" >*</span>
                    </h4>
                    <input type="text" onpaste="return isAlphaNumber(event)" onkeypress="return isAlphaNumber(event)" name="ifscCode" class="inputfileds"  ng-required="true"  ng-model="student.X11_character_IFSC_Code_of_the_Branch__c" maxlength='11' minlength='11' ng-pattern="/^[a-zA-Z0-9]*$/" ng-readonly="editFileds" ng-class="{readInput: editFileds}" ng-change="ifscChanged()"/>
                    <span ng-show="!(formInvalid && basicInfo.ifscCode.$invalid)  && showSpace" class="error">&nbsp;</span>
                    <span ng-show="formInvalid && basicInfo.ifscCode.$invalid" class="error">This is required.</span>
                    <span class="error">&nbsp;</span>
                    <span class="error" ng-if="ifscCodeValid"  style="color: rgb(10, 45, 94);    font-weight: bold;"> {{ifscCodeMessage}}</span>
                    <span class="error" ng-if="ifscCodeValid">&nbsp;</span>
                    
                    <span class="error" ng-show="(!basicInfo.ifscCode.$invalid && basicInfo.ifscCode.$error.minlength)">Invalid PAN details</span>
                    <span class="error" ng-show="!(!basicInfo.ifscCode.$invalid && basicInfo.ifscCode.$error.minlength)  && showSpace">&nbsp;</span>
                    
                    <br/><span ng-if="!editFileds"><input type="button" ng-mouseover="showVerifyIFSChelpText()"  ng-mouseleave="hideVerifyIFSChelpText()" value="Verify IFSC" class="buttons"  ng-click="verifyIfsc(basicInfo.ifscCode);"/>  <span ng-if="ifscCodeValid" style="color: rgb(255, 0, 0);    font-weight: bold;">&nbsp;&nbsp;IFSC code is valid.</span><span ng-if="ifscServiceDisabled" style="color: rgb(255, 0, 0);    font-weight: bold;">&nbsp;&nbsp;Service is disabled.</span><span ng-if="ifscCodeNotValid" style="color: rgb(255, 0, 0);    font-weight: bold;">&nbsp;&nbsp;IFSC code is not valid.</span></span>
                    <span class="error" ng-if="showVerifyIFSChelpTextFlag">&nbsp;</span>
                    <span  class="error"  style="color: rgb(255, 0, 0);    font-weight: bold;" ng-if="showVerifyIFSChelpTextFlag">This helps to verify the IFSC entered by you. This is mandatory. Please check the IFSC code with your bank if there was a merger.</span>
                    
                </div>
                <!--  <div class="row" ng-if="!editFileds">
<h4 class="formTags" >
Use this button to verify the IFSC code:<span style="color:#ff0000" >*</span>
</h4> 
<input type="button" value="Verify IFSC" class="buttons"  ng-click="edit();" ng-if="!editFileds"/>
</div> -->
                <div class="row">
                    <h4 class="tagnames" >
                        Bank Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="bName" ng-model="student.Bank_Name__c"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)" required='true' ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.bName.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.bName.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" style="width:91.5%">
                    <h4 class="formTags" >
                        Branch Address:<span style="color:#ff0000">*</span>
                    </h4>
                    <textarea  name="bAddress" class="inputfileds" required="true"  ng-model="student.Branch_Address__c" style="min-height:98px;width:100%" ng-readonly="editFileds" ng-class="{readInput: editFileds}"></textarea>
                    <span ng-show="formInvalid && basicInfo.bAddress.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.bAddress.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Bank Branch Phone No:
                    </h4>
                    <input type="text" name="bPhone" ng-model="student.Bank_Branch_Phone_No__c"  class="inputfileds" onpaste="return isNumber(event)" onkeypress="return isNumber(event)" ng-readonly="editFileds" maxlength='11' ng-class="{readInput: editFileds}"/>
                </div>
                
                <div class="row">
                    <h4 class="tagnames" >
                        Bank City:
                    </h4>
                    <input type="text" name="bCity" ng-model="student.Bank_City__c"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)"  ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Bank State:
                    </h4>
                    <input type="text" name="bState" ng-model="student.Bank_State__c"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)" ng-readonly="editFileds" ng-class="{readInput: editFileds}"/>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Bank Pincode:
                    </h4>
                    <input type="text" name="bPin" ng-model="student.Bank_Pincode__c"  class="inputfileds" onpaste="return isNumber(event)" onkeypress="return isNumber(event)"  ng-readonly="editFileds" maxlength="6" ng-class="{readInput: editFileds}"/>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Type Of Account:<span style="color:#ff0000">*</span>
                    </h4>
                    <select ng-model="student.Type_Of_Bank_Account__c" class="inputfiledsSelect"  required='true' name="course" ng-disabled="editFileds" style="min-height: 38px;">
                        <option ng-repeat="typeOfBackAcc in typeOfBankAccounts" value="{{typeOfBackAcc}}">{{typeOfBackAcc}}</option>
                    </select>
                    <span ng-show="formInvalid && basicInfo.typeOfBackAcc.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.typeOfBackAcc.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" ng-if="student.Type_Of_Bank_Account__c=='Other'">
                    <h4 class="tagnames" >
                        Type Of Account(Other):<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="Other_Type_Bank_Account__c " ng-model="student.Other_Type_Bank_Account__c "  class="inputfileds" required='true' onpaste="return isChar(event)" onkeypress="return isChar(event)" ng-readonly="editFileds" style="min-height: 38px;" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.Other_Type_Bank_Account__c .$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.Other_Type_Bank_Account__c .$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
            </div>
            <div class="formDiv" >
                <h4 class="formheadings" align="center">
                    Course/College Details    
                </h4>
                <a id="college"></a>
                <div class="row">
                    <h4 class="tagnames" ><a id="college"></a>
                        College Name:<span style="color:#ff0000">*</span>
                    </h4>
                    <md-input-container class="mdContPopup inputfiledsmdSelect">
                        <md-select class="inputfiledsSelect" style="border: 1px solid #ccc;" ng-model="student.College_Name__r.Name" name="college"  ng-disabled="editFileds" ng-change="selectedValue(student.College_Name__r.Name)">
                            <md-select-header class="demo-select-header">
                                <input ng-model="clg" ng-keydown="$event.stopPropagation()" type="search" class="demo-header-searchbox md-text" ng-change="getCollege(clg)"/>
                            </md-select-header>
                            <md-optgroup>  
                                <md-option ng-value="college.Name" ng-repeat="college in colleges">{{college.Name}}</md-option>
                            </md-optgroup>
                        </md-select>
                    </md-input-container>
                    <!--input type="text" name="college" ng-model="student.College_Name__c"  class="inputfileds" onpaste="return isChar(event)" onkeypress="return isChar(event)" required='true'/-->
                    <span ng-show="formInvalid && basicInfo.college.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.college.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" ng-if="student.College_Name__r.Name=='Other'">
                    <h4 class="tagnames" >
                        College Name(Other):<span style="color:#ff0000">*</span>
                    </h4>
                    <input type="text" name="College_Other__c" ng-model="student.College_Other__c"  class="inputfileds" required='true' onpaste="return isChar(event)" onkeypress="return isChar(event)" ng-readonly="editFileds" style="min-height: 38px;" ng-class="{readInput: editFileds}"/>
                    <span ng-show="formInvalid && basicInfo.College_Other__c.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.College_Other__c.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row">
                    <h4 class="tagnames" >
                        Course:<span style="color:#ff0000">*</span>
                    </h4>
                    <select ng-model="student.Course__c" class="inputfiledsSelect"  required='true' name="course" ng-disabled="editFileds" style="min-height: 38px;">
                        <option ng-repeat="course in studentCourse" value="{{course}}">{{course}}</option>
                    </select>
                    <span ng-show="formInvalid && basicInfo.course.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.course.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" ng-if="student.Course__c !='MBBS' && student.Course__c !='BPharm' ">
                    <h4 class="tagnames" >
                        Branch:<span style="color:#ff0000">*</span>
                    </h4>
                    <select ng-model="student.Branch_Stream__c" class="inputfiledsSelect" required="student.Course__c!='MBBS' || student.Course__c !='BPharm'" name="branch" ng-disabled="student.Course__c=='MBBS' || editFileds || student.Course__c =='BPharm' " style="min-height: 38px;">
                        <option ng-repeat="studentBranch in StudentBranches" value="{{studentBranch}}" ng-if="student.Course__c=='BE/B.Tech' || student.Course__c=='Integrated MTech'">{{studentBranch}}</option>
                        <option ng-repeat="studentBranch in StudentLawBranches" value="{{studentBranch}}" ng-if="student.Course__c=='Law'">{{studentBranch}}</option>
          
                    </select>
                    <span ng-show="formInvalid && basicInfo.branch.$invalid" class="error">This is required.</span>
                    <span ng-show="!(formInvalid && basicInfo.branch.$invalid) && showSpace" class="error">&nbsp;</span>
                </div>
                <div class="row" ng-if="student.Branch_Stream__c=='Other' && student.Course__c !='MBBS'">
                    <h4 class="tagnames" >
