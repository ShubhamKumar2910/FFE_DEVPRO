            var windowClose             = "{!JSENCODE($Label.Scholarship_application_window_closed_msg)}";  
            var isPrelimAvailable       = {!isPrelimAvailable};
            //Added Scholarship Criteria Values - Sumit Gaurav - 11-June-2020
