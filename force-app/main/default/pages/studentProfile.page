                    <apex:image url="{!$Resource.close_Pop}" styleClass="materialIcons closeIcon" />
                </div>
                <div class="uploadDocsDiv">
                    <div class="row" style="font-weight: bold;margin-top: 8px;    width: 47%;" >  
                        <label >{{replaceAttachments.Name}}</label>
                    </div>
                    <div class="row" style="    width: 47%;">    
                        <input type="file" name="doc1"  class="inputfileds" id="{{replaceAttachments.Id}}" required="true" valid-file='true'/>
                    </div>
                </div>
                <div class="row" style="font-weight: bold;" ng-if="replaceAttachments.Name=='Income Certificate, Salary Certificate, Pension Certificate or Income Tax Return or Other document, as applicable' && student.Father_Occupation__c != 'Service'">  
                    <h4 class="tagnames" >
                        Certificate Expiry Date:<span style="color:#ff0000">*</span>
                    </h4>
                    <md-datepicker class="inputfiledsSelect" type="date" ng-model="student.Certificate_Expiry_Date__c" name="certificateExpiryDate" id="certificateExpiryDate"  md-min-date='minDate' onkeydown="return false" ng-change="checkCertificateDate(student.Certificate_Expiry_Date__c)"></md-datepicker>                                       
                </div> 
                <!-- EXPENSE DETAILS -->
                <div class="row" ng-if="replaceAttachments.Name=='Receipts for Actual Expenses incurred for current year(College Fee/Hostel & Mess/Transport/Books/Uniform)' || replaceAttachments.Name=='Expense Receipts for Actual Expenses incurred for current academic year(College Fee/Hostel & Mess/Transport/Books/Uniform)'">
                 <h4 class="formheadings" align="center">
                        Actual Expense/Fee Details     
                    </h4>
                    <div class="row">
                        <h4 class="tagnames">
                            Actual College Fees<span style="color:#ff0000;font-size:18px;">*</span>:
                        </h4>
                        <input type="text" value="0" name="Student_College_Fees__c" ng-model="activeAppli.Student_College_Fees__c"  class="inputfileds" onpaste="return isNumber(event)" onkeypress="return isNumber(event)" maxlength="6"/>
                    </div>
                    <div class="row">
                        <h4 class="tagnames" >
                            Actual Hostel/Mess Expenses<span style="color:#ff0000;font-size:18px;">*</span>:
                        </h4>
                        <input type="text" name="Student_Hostel_Mess_Expenses__c" ng-model="activeAppli.Student_Hostel_Mess_Expenses__c"  class="inputfileds " onpaste="return isNumber(event)" onkeypress="return isNumber(event)" maxlength="6"/>
                    </div>
                    <div class="row">
                        <h4 class="tagnames" >
                            Actual Books Expenses<span style="color:#ff0000;font-size:18px;">*</span>:
                        </h4>
                        <input type="text" name="Student_Books_Expenses__c" ng-model="activeAppli.Student_Books_Expenses__c"  class="inputfileds " onpaste="return isNumber(event)" onkeypress="return isNumber(event)" maxlength="6"/>
                    </div>
                    <div class="row">
                        <h4 class="tagnames" >
                            Actual Transportation Expenses<span style="color:#ff0000;font-size:18px;">*</span>:
                        </h4>
                        <input type="text" name="Student_Transportation_Expenses__c" ng-model="activeAppli.Student_Transportation_Expenses__c"  class="inputfileds "  onpaste="return isNumber(event)" onkeypress="return isNumber(event)" maxlength="6"/>
                    </div>
                    <div class="row">
                        <h4 class="tagnames" >
                            Actual Uniform Expenses<span style="color:#ff0000;font-size:18px;">*</span>:
